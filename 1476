#날짜 계산
#E는 16년 주기, M은 29년 주기, S는 20년 주기로 리셋


#include <iostream>
using namespace std;
int main(){
    int x, y, z;
    cin >> x >> y >> z;
    int E=1, M=1, S=1, result=1;
    while(1){
        if(E==x&&M==y&&S==z)
            break;
        E++;
        M++;
        S++;
        result++;
	    if(E==16){E=1;}
	    if(M==29){M=1;}
	    if(S==20){S=1;}
        
    }
    cout << result;
}

	
